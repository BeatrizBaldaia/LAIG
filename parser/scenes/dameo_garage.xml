        <LIGHT id="light0">
            <enable value ="1" />
            <position  x="5" y="30" z="5" w="1" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r=".4" g=".4" b=".4" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </LIGHT>
        <LIGHT id="light1">
            <enable value ="0" />
            <position  x="30" y="30" z="15" w="1" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r=".4" g=".4" b=".4" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </LIGHT>
    </LIGHTS>
    <TEXTURES>
        <TEXTURE id="board">
            <file path="images_garage/board.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="board_wood">
            <file path="images_garage/board_wood.jpg" />
            <amplif_factor s="1" t="1" />
