        </TEXTURE>
        <TEXTURE id="metal1">
            <file path="images_garage/metal1.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="metal2">
            <file path="images_garage/metal2.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="gold">
            <file path="images_garage/gold.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="wall">
            <file path="images_garage/wall.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="wood">
            <file path="images_garage/wood.jpg" />
            <amplif_factor s="1" t="1" />
