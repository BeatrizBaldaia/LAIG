        <ANIMATION id="lever_down" speed="1.5" type="rotx" ang="90">
        </ANIMATION>
        <ANIMATION id="lever_up" speed="1.5" type="rotx" ang="-90">
        </ANIMATION>
        <ANIMATION id="buton_on" speed="1.5" type="transz" dist="-0.04">
        </ANIMATION>
        <ANIMATION id="buton_off" speed="1.5" type="transz" dist="0.04">
        </ANIMATION>
    </ANIMATIONS>
