<SCENE>

	<!-- Initial values -->
	<INITIALS>
		<frustum near="0.1" far="500"/>
		<translation x="0" y="0" z="-4"/>
		<rotation axis="x" angle="0" />
		<rotation axis="y" angle="0" />
		<rotation axis="z" angle="0" />
		<scale sx="0.25" sy="0.25" sz="0.25" />
		<reference length="5" />
	</INITIALS>

	<!-- Illumination and clear color -->
	<ILLUMINATION>
		<ambient r="0.2" g="0.2" b="0.2" a="1" />
		<background r="0" g="0" b="0" a="1" />
	</ILLUMINATION>

	<!-- Lights -->
	<LIGHTS>
		<LIGHT id="light0">
			<enable value ="1" />
			<position  x="0" y="10" z="0" w="1" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</LIGHT>
	</LIGHTS>

	<TEXTURES>
		<!-- Rocks -->
		<TEXTURE id="bank">
			<file path="images/bank0.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<!-- New york floor -->
		<TEXTURE id="nwfloor">
			<file path="images/nwfloor.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<!-- Tower face -->
		<TEXTURE id="twface">
			<file path="images/skyscraper.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<TEXTURE id="goodyear">
			<file path="images/goodyear.png" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<TEXTURE id="red_plastic">
			<file path="images/red.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="hel_metal">
			<file path="images/metal.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="grey_metal">
			<file path="images/black_plastic.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="black_metal">
			<file path="images/leaves0.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>
	</TEXTURES>

	<MATERIALS>
		<MATERIAL id="piramide">
			<shininess value="1" />
			<specular r="0" g="0" b="0" a="1" />
			<diffuse r="1.0" g="0.68" b="0.4" a="1" />
			<ambient r="0.2" g="0.2" b="0.2" a="1" />
			<emission r="0.0" g="0.0" b="0.0" a="1" />
		</MATERIAL>
	</MATERIALS>

	<!-- The nodes -->
	<NODES>
		<ROOT id="root" />

		<NODE id="root">
			<MATERIAL id="piramide"/>
			<TEXTURE id="bank"/>

			<DESCENDANTS>
				<NODEREF id="zeppelin"/>
				<NODEREF id="helicopter"/>
				<NODEREF id="tower"/>
				<NODEREF id="floor" />
			</DESCENDANTS>

		</NODE>

		<NODE id="zeppelin">
			<MATERIAL id="piramide"/>
			<TEXTURE id="goodyear"/>

			<TRANSLATION x="10" y="10" z="10"/>

			<DESCENDANTS>
				<NODEREF id="ballon"/>
				<NODEREF id="pads"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="pads" >
			<MATERIAL id="piramide"/>
			<TEXTURE id="red_plastic"/>

			<DESCENDANTS>
				<NODEREF id="pads_vert"/>
				<NODEREF id="pads_hor"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="pads_vert" >
			<MATERIAL id="piramide"/>
			<TEXTURE id="null"/>

			<DESCENDANTS>
				<NODEREF id="lower_pad"/>
				<NODEREF id="upper_pad"/>
