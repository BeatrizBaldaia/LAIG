<SCENE>

    <!-- Initial values -->
    <INITIALS>
        <frustum near="0.1" far="500"/>
        <translation x="-25" y="-25" z="-25" />
        <rotation axis="x" angle="0" />
        <rotation axis="y" angle="10" />
        <rotation axis="z" angle="0" />
        <scale sx="1.0" sy="1.0" sz="1.0" />
        <reference length="10" />
    </INITIALS>


    <!-- Illumination and clear colour -->
    <ILLUMINATION>
        <ambient r="0.2" g="0.2" b="0.2" a="1" />
        <doubleside value="1" />
        <background r="0.10588" g="0.2" b="0.51372" a="1" />
    </ILLUMINATION>


    <!-- Lights -->
    <LIGHTS>
        <LIGHT id="light0">
            <enable value ="1" />
            <position  x="0" y="10" z="0" w="1" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light1">
            <enable value ="1" />
            <position x="30" y="10" z="0" w="1" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light2">
            <enable value ="1" />
            <position x="0" y="10" z="20" w="1" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light3">
            <enable value ="1" />
            <position x="30" y="10" z="20" w="1" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.6" g="0.6" b="0.6" a="1.0" />
        </LIGHT>

    </LIGHTS>

    <!-- Textures -->
    <TEXTURES>
        <!-- light_metal -->
        <TEXTURE id="light_metal">
            <file path="images/metal1.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

        <!-- dark_metal -->
        <TEXTURE id="dark_metal">
            <file path="images/metal2.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

        <!-- red_metal -->
        <TEXTURE id="red_metal">
            <file path="images/red_metal.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

        <!-- blue_metal -->
        <TEXTURE id="blue_metal">
            <file path="images/blue_metal.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

        <!-- star -->
        <TEXTURE id="star">
            <file path="images/star.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

    </TEXTURES>


    <MATERIALS>

        <!-- Rocket material -->
        <MATERIAL id="bigRocketMaterial">
            <shininess value="1" />
            <specular r="0.6" g="0.6" b="0.6" a="1" />
            <diffuse r="0.84313" g="0.84313" b="0.84313" a="1" />
            <ambient r="0.2" g="0.2" b="0.2" a="1" />
            <emission r="0.0" g="0.0" b="0.0" a="1" />
        </MATERIAL>

        <!-- platform material -->
        <MATERIAL id="platformMaterial">
            <shininess value="1" />
            <specular r="0.6" g="0.6" b="0.6" a="1" />
            <diffuse r="0.45882" g="0.670588" b="0.949019" a="1" />
            <ambient r="0.2" g="0.2" b="0.2" a="1" />
            <emission r="0.0" g="0.0" b="0.0" a="1" />
        </MATERIAL>

        <!-- Star material -->
        <MATERIAL id="starMaterial">
            <shininess value="100" />
            <specular r="0.85" g="0.85" b="0.85" a="1" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1" />
            <ambient r="0.9" g="0.9" b="0.9" a="1" />
            <emission r="0.9" g="0.9" b="0.9" a="1" />
        </MATERIAL>

        <!-- A default material -->
        <MATERIAL id="defaultMaterial">
            <shininess value="1" />
            <specular r="0.0" g="0.0" b="0.0" a="1" />
            <diffuse r="0.4" g="0.4" b="0.4" a="1" />
            <ambient r="0.4" g="0.4" b="0.4" a="1" />
            <emission r="0.0" g="0.0" b="0.0" a="1" />
        </MATERIAL>

    </MATERIALS> 

<!-- The leaves -->
<LEAVES>
</LEAVES>

    <!-- The nodes -->
    <NODES>

        <ROOT id="root" />

        <!-- The root node -->
        <NODE id="root">
            <MATERIAL id="defaultMaterial" />
            <TEXTURE id="null" />

            <DESCENDANTS>
                <NODEREF id="platform" />
                <NODEREF id="stars" />
                <NODEREF id="bigRocket" /> 
            </DESCENDANTS>

        </NODE>

        <!-- The platform box -->
        <NODE id="platform">
            <MATERIAL id="platformMaterial" />
            <TEXTURE id="blue_metal" />

            <TRANSLATION x="3" y="0" z="3" />
            <SCALE sx="6" sy="1" sz="4" />
            
            <DESCENDANTS>

                <NODEREF id="facePlatform1" />
                <NODEREF id="facePlatform2" />
                <NODEREF id="facePlatform3" />
                <NODEREF id="facePlatform4" />
                <NODEREF id="facePlatform5" />
                <NODEREF id="facePlatform6" />
            </DESCENDANTS>

        </NODE>

        <!-- THIS BLOCK OF CODE WILL CREATE A BOX (SIZE 1) CENTERED IN THE ORIGIN -->

        <NODE id="facePlatform1">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <ROTATION axis="x" angle="180" />
            <TRANSLATION x="0" y="-1" z="0" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 1 1 0" />
            </DESCENDANTS>

        </NODE>

        <NODE id="facePlatform2">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="0" y="0" z="1" />


            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 1 1 0" />
            </DESCENDANTS>

        </NODE>

        <NODE id="facePlatform3">
            <MATERIAL id="null" />
            <TEXTURE id="null" />


            <ROTATION axis="y" angle="90" />
            <TRANSLATION x="-1" y="0" z="1" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 1 1 0" />
            </DESCENDANTS>

        </NODE>

        <NODE id="facePlatform4">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <ROTATION axis="y" angle="-90" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 1 1 0" />
            </DESCENDANTS>

        </NODE>

        <NODE id="facePlatform5">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <ROTATION axis="x" angle="90" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 1 1 0" />
            </DESCENDANTS>

        </NODE>

        <NODE id="facePlatform6">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <ROTATION axis="x" angle="-90" />
            <TRANSLATION x="0" y="-1" z="1" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 1 1 0" />
            </DESCENDANTS>

        </NODE>

        <!-- END OF CUBE -->

        <!-- The stars -->

        <NODE id="stars">
            <MATERIAL id="starMaterial" />
            <TEXTURE id="star" />

            <DESCENDANTS>
                <NODEREF id="star1" />
                <NODEREF id="star2" />
                <NODEREF id="star3" />
                <NODEREF id="star4" />
                <NODEREF id="star5" />
            </DESCENDANTS>

        </NODE>

        <NODE id="star1">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="15" y="10" z="0" />
            <!-- <SCALE sx="2" sy="2" sz="2" /> -->

            <DESCENDANTS>
                <NODEREF id="star" />
            </DESCENDANTS>

        </NODE>

        <NODE id="star2">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="-10" y="5" z="3" />
            <!-- <SCALE sx="2" sy="2" sz="2" /> -->

            <DESCENDANTS>
                <NODEREF id="star" />
            </DESCENDANTS>

        </NODE>

        <NODE id="star3">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="-8" y="9" z="8" />
            <!-- <SCALE sx="2" sy="2" sz="2" /> -->

            <DESCENDANTS>
                <NODEREF id="star" />
            </DESCENDANTS>

        </NODE>

        <NODE id="star4">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="4" y="3" z="9" />
            <!-- <SCALE sx="2" sy="2" sz="2" /> -->

            <DESCENDANTS>
                <NODEREF id="star" />
            </DESCENDANTS>

        </NODE>

        <NODE id="star5">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="6" y="5" z="0" />
            <!-- <SCALE sx="2" sy="2" sz="2" /> -->

            <DESCENDANTS>
                <NODEREF id="star" />
            </DESCENDANTS>

        </NODE>

        <NODE id="star">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <DESCENDANTS>
                <LEAF id="sphere1" type="sphere" args="1 30 30" />

            </DESCENDANTS>

        </NODE>

        <NODE id="bigRocket">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="6" y="1" z="5" />

        <DESCENDANTS>
            <NODEREF id="bottomBody" />
            <NODEREF id="topBody" /> 
        </DESCENDANTS>

    </NODE>

        <NODE id="bottomBody">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <SCALE sx="1" sy="5" sz="1" />
        <DESCENDANTS>
            <NODEREF id="cylinderBody" /> 
            <NODEREF id="lowerStageCover" />
            <NODEREF id="wings" />

        </DESCENDANTS>

    </NODE>

        <NODE id="topBody">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <SCALE sx="0.7" sy="1.2" sz="0.7" /> -->
        <TRANSLATION x="0" y="5" z="0" />
        <DESCENDANTS>
            <NODEREF id="upperStageBody" />
            <NODEREF id="upperStageCover" />

        </DESCENDANTS>

    </NODE>

        <NODE id="lowerStageCover">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <TRANSLATION x="0" y="1" z="0" />

        <DESCENDANTS>
            <NODEREF id="coneBody" />

        </DESCENDANTS>

    </NODE>

        <NODE id="upperStageCover">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <TRANSLATION x="0" y="2.5" z="0" />

        <DESCENDANTS>
            <NODEREF id="ovalConeBody" />

        </DESCENDANTS>

    </NODE>

        <NODE id="upperStageBody">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <SCALE sx="1" sy="2.5" sz="1" />

        <DESCENDANTS>
            <NODEREF id="cylinderBody" />

        </DESCENDANTS>

    </NODE>

        <NODE id="cylinderBody">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="light_metal" />

        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <LEAF id="cylinder1" type="cylinder" args="1 1 1 30 30" />

        </DESCENDANTS>

    </NODE>

        <NODE id="coneBody">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="dark_metal" />

        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <LEAF id="cone1" type="cylinder" args="1 1 0 30 30" />

        </DESCENDANTS>

    </NODE>

        <NODE id="ovalConeBody">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="dark_metal" />
        <SCALE sx="1" sy="1.5" sz="1" />
        <DESCENDANTS>
            <LEAF id="cone1" type="sphere" args="1 30 30" />

        </DESCENDANTS>

    </NODE>


    <NODE id="wings">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />

        <DESCENDANTS>
            <NODEREF id="wing1" /> 
            <NODEREF id="wing2" />
        </DESCENDANTS>

    </NODE>

    <NODE id="wing1">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />

        <TRANSLATION x="-1.5" y="0" z="-0.2" />

        <DESCENDANTS>
            <NODEREF id="wingStruct" />
        </DESCENDANTS>

    </NODE>

    <NODE id="wing2">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />
        <ROTATION axis="y" angle="180" />

        <TRANSLATION x="-1.5" y="0" z="-0.1" />

        <DESCENDANTS>
            <NODEREF id="wingStruct" />
        </DESCENDANTS>

    </NODE>

    <NODE id="wingStruct">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />

        <SCALE sx="0.5" sy="0.3" sz="0.2" />

        <DESCENDANTS>

            <NODEREF id="wingFace1" />
            <NODEREF id="wingFace2" />
            <NODEREF id="wingFace3" />
            <NODEREF id="wingFace4" />
            <NODEREF id="wingFace5" /> 
        </DESCENDANTS>

    </NODE>

    <NODE id="wingFace1">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />
        
        <TRANSLATION x="0" y="0" z="1" />

        <DESCENDANTS>
            <LEAF id="triangle1" type="triangle" args="0 0 0 1 0 0 1 1 0" />

        </DESCENDANTS>

        </NODE>

    <NODE id="wingFace2">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />
        <ROTATION axis="y" angle="-180" />
        

        <DESCENDANTS>
            <LEAF id="triangle1" type="triangle" args="0 0 0 -1 1 0 -1 0 0" />

        </DESCENDANTS>

    </NODE>

    <NODE id="wingFace3">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />
        <ROTATION axis="x" angle="90" />

        <DESCENDANTS>
            <LEAF  type="rectangle" args="0 1 1 0" />

        </DESCENDANTS>

    </NODE>

    <NODE id="wingFace4">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />
        <ROTATION axis="y" angle="90" />
        <TRANSLATION x="-1" y="0" z="1" />

        <DESCENDANTS>
            <LEAF  type="rectangle" args="0 1 1 0" />

        </DESCENDANTS>

    </NODE>

    <NODE id="wingFace5">
        <MATERIAL id="bigRocketMaterial" />
        <TEXTURE id="red_metal" />
        <ROTATION axis="y" angle="-90" />
        <ROTATION axis="x" angle="-45" />
        <SCALE sx="1" sy="1.4142135" sz="4" />

        <DESCENDANTS>
            <LEAF  type="rectangle" args="0 1 1 0" />

        </DESCENDANTS>

    </NODE>
    </NODES>

</SCENE>