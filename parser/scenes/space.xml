<SCENE>

    <!-- Initial values -->
    <INITIALS>
        <frustum near="0.001" far="500"/>
        <translation x="-1" y="1" z="0" />
        <rotation axis="x" angle="0" />
        <rotation axis="y" angle="0" />
        <rotation axis="z" angle="0" />
        <scale sx=".5" sy=".5" sz=".5" />
        <reference length="10" />
    </INITIALS>


    <!-- Illumination and clear colour -->
    <ILLUMINATION>
        <ambient r="0.2" g="0.2" b="0.2" a="1" />
        <doubleside value="1" />
        <background r="0.10588" g="0.2" b="0.51372" a="1" />
    </ILLUMINATION>


    <!-- Lights -->
    <LIGHTS>
        <LIGHT id="light0">
            <enable value ="1" />
            <position  x="0" y="10" z="0" w="1" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r=".7" g=".7" b=".7" a="1" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light1">
            <enable value ="1" />
            <position x="30" y="10" z="0" w="1" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r=".7" g=".70" b=".7" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light2">
            <enable value ="1" />
            <position x="0" y="10" z="20" w="1" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r=".5" g=".5" b=".5" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light3">
            <enable value ="1" />
            <position x="30" y="10" z="20" w="1" />
            <ambient r="0" g="0" b="0" a="0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.6" g="0.6" b="0.6" a="1.0" />
        </LIGHT>

    </LIGHTS>

    <!-- Textures -->
    <TEXTURES>
        <!-- light_metal -->
        <TEXTURE id="body">
            <file path="images/bodyBigRocket.png" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <!-- chess -->
        <TEXTURE id="chess">
            <file path="images/chess.png" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

        <!-- red_metal -->
        <TEXTURE id="red_metal">
            <file path="images/red_metal.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>

        <!-- target -->
        <TEXTURE id="white_metal">
            <file path="images/white_metal.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <!-- star -->
        <TEXTURE id="star">
            <file path="images/star.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="fire_le">
            <file path="images/fire_le.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="foguetao">
            <file path="images/export.png" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
    </TEXTURES>


    <MATERIALS>

        <!-- Rocket material -->
        <MATERIAL id="bigRocketMaterial">
            <shininess value="1" />
            <specular r="0.6" g="0.6" b="0.6" a="1" />
            <diffuse r="0.84313" g="0.84313" b="0.84313" a="1" />
            <ambient r="0.2" g="0.2" b="0.2" a="1" />
            <emission r="0.0" g="0.0" b="0.0" a="1" />
        </MATERIAL>

        <!-- platform material -->
        <MATERIAL id="platformMaterial">
            <shininess value="1" />
            <specular r="0.6" g="0.6" b="0.6" a="1" />
            <diffuse r="0.45882" g="0.670588" b="0.949019" a="1" />
            <ambient r="0.2" g="0.2" b="0.2" a="1" />
            <emission r="0.0" g="0.0" b="0.0" a="1" />
        </MATERIAL>

        <!-- Star material -->
        <MATERIAL id="starMaterial">
            <shininess value="100" />
            <specular r="0.85" g="0.85" b="0.85" a="1" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1" />
            <ambient r="0.9" g="0.9" b="0.9" a="1" />
            <emission r="0.9" g="0.9" b="0.9" a="1" />
        </MATERIAL>
        <MATERIAL id="fire">
            <shininess value="100" />
            <specular r="0.85" g="0.85" b="0.85" a="1" />
            <diffuse r="1.0" g=".1" b=".1" a="1" />
            <ambient r="1" g="0.1" b="0.1" a="1" />
            <emission r="1" g="1" b="1" a="1" />
        </MATERIAL>

        <!-- A default material -->
        <MATERIAL id="defaultMaterial">
            <shininess value="1" />
            <specular r="0.0" g="0.0" b="0.0" a="1" />
            <diffuse r="0.4" g="0.4" b="0.4" a="1" />
            <ambient r="0.4" g="0.4" b="0.4" a="1" />
            <emission r="0.0" g="0.0" b="0.0" a="1" />
        </MATERIAL>

    </MATERIALS> 

<!-- The leaves -->
<LEAVES>
</LEAVES>

